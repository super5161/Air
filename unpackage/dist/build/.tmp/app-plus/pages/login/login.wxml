<view class="content"><view class="logo-content"><image class="logo" src="/static/image/log.png"></image><view class="logo-text">{{res.sysName}}</view></view><view class="form-content"><view class="input-group"><label for="tenantId">{{res.lbltenantId}}</label><input name="tenantId" type="text" maxlength="10" placeholder="{{res.placTenantId}}" data-event-opts="{{[['input',[['__set_model',['','tenantId','$event',[]]]]]]}}" value="{{tenantId}}" bindinput="__e"/></view><view class="input-group"><label for="username">{{res.lblUserName}}</label><input name="username" type="text" maxlength="10" placeholder="{{res.placUserName}}" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/></view><view class="input-group"><label for="password"><rich-text nodes="{{res.lblPassword}}"></rich-text></label><input name="password" maxlength="10" password="true" type="text" placeholder="{{res.placPassword}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="input-group"><label><rich-text nodes="{{res.lblLange}}"></rich-text></label><text data-event-opts="{{[['tap',[['ChangeLange',['$event']]]]]}}" bindtap="__e">{{langText}}</text></view><button type="primary" data-event-opts="{{[['tap',[['Login',['$event']]]]]}}" bindtap="__e">登录</button></view><view class="version" style="{{(versionTop)}}">Copyright © 1996-2020 轶伦.版权所有 ver 1.0</view></view>