
.head {
	width: 100%;
	height: 400rpx;
	background-color: #336e7b;
	color: #FFFFFF;
}
.user-fa {
	float: left;
	height: 400rpx;
	width: 250rpx;
}
.user-fa .warpper {
	width: 200rpx;
	height: 200rpx;
	margin: 0 30rpx;
	margin-top: 200rpx;
	-webkit-transform: translateY(-100rpx);
	    -ms-transform: translateY(-100rpx);
	        transform: translateY(-100rpx);
	border-radius: 50%;
}
.user-fa .warpper ._img {
	height: 100%;
	width: 100%;
	border-radius: 50%;
}
.user-info {
	padding-top: 100rpx;
	margin-left: 260rpx;
}
.user-info .title {
	font-size: 25px;
}
.user-info .desc {
	font-size: 16px;
}

