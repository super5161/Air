
page,
.content {
	width: 100%;
	height: 100%;
	background-size: 100% auto;
	padding: 0;
	background: #FFFFFF;
}
.swiper {
	width: 100%;
	height: 100%;
}
.sw-image {
	width: 100%;
	height: 100%;
}
.sw-title {
	color: #FFFFFF;
	position: fixed;
	bottom: 300rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	    -ms-transform: translateX(-50%);
	        transform: translateX(-50%);
	font-size: 50rpx;
}
.jump-over,
.experience {
	position: absolute;
	height: 60rpx;
	line-height: 60rpx;
	padding: 0 40rpx;
	border-radius: 30rpx;
	font-size: 32rpx;
	color: #454343;
	border: 1px solid #FFFFFF;
	z-index: 999;
}
.jump-over {
	right: 50rpx;
	top: 100rpx;
	color: #FFFFFF;
}
.experience {
	right: 50%;
	margin-right: -105rpx;
	color: #FFFFFF;
	bottom: 150rpx;
}

